            "get": {
                "tags": [
                    "Reset Password"
                ],
                "summary": "Check if exist token",
                "description": "Returns list of Communities",
                "operationId": "check_token_password",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Bad request"
                    }
                },
                "security": [
                    {
                        "api_key_security_example": []
                    }
                ]
            }
        },
        "/password/create": {
            "post": {
                "tags": [
                    "Reset Password"
                ],
                "summary": "Envia um email para resetar a senha",
                "description": "Envia um email para resetar a senha",
                "operationId": "reset_password_request",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "text"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "email": "teste@gmail.com"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Bad request"
                    }
                },
                "security": [
                    {
                        "api_key_security_example": []
                    }
                ]
            }
        },
        "/password/reset": {
            "post": {
                "tags": [
                    "Reset Password"
                ],
                "summary": "Form reset password",
                "description": "Formulário de resetar a senha",
                "operationId": "reset_password",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "token",
                                    "password",
                                    "password_confirmation"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "text"
                                    },
                                    "token": {
                                        "type": "text"
                                    },
                                    "password": {
                                        "type": "text"
                                    },
                                    "password_confirmation": {
                                        "type": "text"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "email": "wesleymotabr@gmail.com",
